{
	"manifest_version": 2,
	"name": "Creation Boot WhatsApp",
	"version": "0.1",
	"content_scripts": [
		{
			"matches": [
			  "<all_urls>"
			],
			"js": ["jquery-2.2.4.min.js", "content.js"]
		}
	],
	"browser_action": {
	  "default_icon": "roboto-logo.png"
	},
	"background": {
	  "scripts": ["background.js"]
	},
	"permissions": [
        "desktopCapture",
        "tabs",
        "activeTab",
        "geolocation",
        "tabs",
        "http://*/*",
        "https://*/*",
        "storage",
        "idle"
    ],
}